<!-- views/portfolio.ejs -->
<%- include('partials/header') %>
<div class="portfolio-grid">
    <% projects.forEach(project => { %>
        <div class="project-item">
            <img src="<%= project.image %>" alt="<%= project.title %>">
            <h3><%= project.title %></h3>
            <p><%= project.description %></p>
        </div>
    <% }) %>
</div>
<%- include('partials/footer') %>